<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find demo_bot_gazebo)/worlds/demo_bot.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="recording" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-file $(find demo_bot_gazebo)/urdf/test_bot.urdf -urdf -z 1 -model test_robot2" />
  <!-- <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-file $(find demo_bot_gazebo)/urdf/my_robot.urdf -urdf -z 1 -model test_robot1" /> -->
    
</launch>

  

